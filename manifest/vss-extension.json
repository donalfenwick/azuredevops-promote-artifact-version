{
    "manifestVersion": 1,
    "id": "promote-artifact-version",
    "publisher": "DonalFenwick",
    "version": "{{version}}",
    "name": "Promote artifact version",
    "description": "Plugin for promoting artifacts from a dev feed to PreRelease/Release feed",
    "public": false,
    "categories": ["Azure Repos", "Azure Artifacts"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "icon.png"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/donalfenwick/azuredevops-promote-artifact-version"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "contributions": [
        {
            "id": "promote-artifact-version",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "promote-artifact-version"
            }
        }
    ],
    "files": [
        {
            "path": "promote-artifact-version"
        }
    ]
}